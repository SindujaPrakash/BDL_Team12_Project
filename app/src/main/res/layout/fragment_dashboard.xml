<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_rectangle">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="133dp"
        android:textSize="50px"
        android:textColor="#ffffff"
        android:fontFamily="@font/typo_round_bold_demo"
        android:layout_marginStart="133dp"
        android:layout_marginTop="45dp"
        android:text="your body" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:fontFamily="@font/typo_round_bold_demo"
        android:text="in real time"
        android:textColor="#ffffff"
        android:textSize="30px" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="157dp"
        android:text="chill, you're fine"
        android:fontFamily="@font/typo_round_bold_demo"
        android:textColor="#ffffff"
        android:textSize="30px"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="79dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="82dp"
        android:layout_marginLeft="51dp"
        android:layout_marginStart="51dp"
        android:text="heart beat"
        android:fontFamily="@font/typo_round_bold_demo"
        android:textColor="#ffffff"
        android:textSize="30px"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView7"
        android:layout_toEndOf="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:text="breathanalyzer"
        android:fontFamily="@font/typo_round_bold_demo"
        android:textColor="#ffffff"
        android:textSize="30px"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7"
        android:layout_below="@+id/textView7"
        android:layout_marginTop="-82dp"
        android:text="150 bpm"
        android:fontFamily="@font/typo_round_bold_demo"
        android:textColor="#ffffff"
        android:textSize="30px"/>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView9"
        android:layout_marginEnd="71dp"
        android:layout_marginRight="71dp"
        android:text="normal"
        android:fontFamily="@font/typo_round_bold_demo"
        android:textColor="#ffffff"
        android:textSize="30px"/>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="51dp"
        app:srcCompat="@drawable/heartgif" />


</RelativeLayout>
